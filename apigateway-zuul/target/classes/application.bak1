server:
  port: 10900 #程序启动后的端口，也就是tomcat的端口，可以自己定义
spring:
  application:
    name: apigateway-zuul #应用名称，别名
eureka:
  client:
    service-url:
      defaultZone: http://user:123@localhost:20000/eureka
  instance:
    prefer-ip-address: true
security:
  user:
    name: user
    password: 123
zuul:
  routes:
    abcdef: #唯一名称
      path: /abcd/**  #映射的路径
      url: http://169.254.79.136:7900/
